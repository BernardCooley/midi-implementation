generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  midiChannels MidiChannel[]
}

model MidiChannel {
  id      Int      @id @default(autoincrement())
  channel Int
  device String
  parameter String
  userId  Int
  port String
  user User @relation(fields: [userId], references: [id])
}
